{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": ["node"],
    "lib": ["dom", "es6"],
    "noImplicitAny":false,
    "allowUnreachableCode":true,
    "allowUnusedLabels":true,
    "allowJs":true,
    "noUnusedLocals":false
  }
}
